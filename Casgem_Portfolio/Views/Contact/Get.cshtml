﻿@model List<Casgem_Portfolio.Models.Entities.TblContact>
@{
    ViewBag.Title = "Get";
    Layout = "~/Views/Shared/_PortfolioAdminLayout.cshtml";
}

<h2>İletişim Bilgileri</h2>
<table class="table table-bordered">
    <tr>
        <th>#</th>
        <th>Ad Soyad</th>
        <th>Mail</th>
        <th>Telefon</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.ContactID</td>
            <td>@item.NameSurname</td>
            <td>@item.Email</td>
            <td>@item.Telephone</td>
        </tr>
    }
</table>